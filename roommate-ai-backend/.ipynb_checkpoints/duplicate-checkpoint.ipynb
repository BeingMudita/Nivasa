    "        \"sharing_diff\": abs(u1[\"sharing\"] - u2[\"sharing\"]),\n",
    "        \"compatibility_score\": calculate_compatibility(u1, u2) / 100\n",
    "    }\n",
    "    pairs.append(pair)\n",
    "\n",
    "df_pairs = pd.DataFrame(pairs)"
